{
  "name": "gemini-core",
  "version": "6.3.6",
  "description": "Utility which contains common modules for gemini and hermione",
  "main": "build/index.js",
  "bugs": "https://github.com/gemini-testing/gemini-core/issues",
  "scripts": {
    "lint": "eslint .",
    "build-core": "tsc -p tsconfig.json",
    "build-client": "tsc -p ./lib/browser/client-scripts/tsconfig.json",
    "build": "run-s build-core build-client",
    "postbuild": "npm run prepare-calibrate-script",
    "prepare-calibrate-script": "uglifyjs ./build/lib/browser/client-scripts/calibrate.js -m > ./build/lib/browser/client-scripts/calibrate.min.js --support-ie8",
    "test-unit": "mocha test",
    "pretest": "npm run build",
    "test": "run-s lint test-unit",
    "prepublishOnly": "npm run build",
    "toc": "doctoc README.md"
  },
  "engines": {
    "node": ">= 8"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/gemini-testing/gemini-core.git"
  },
  "keywords": [
    "gemini",
    "hermione",
    "core"
  ],
  "dependencies": {
    "aliasify": "^1.9.0",
    "bluebird": "^3.4.6",
    "browserify": "^13.3.0",
    "debug": "^2.2.0",
    "gemini-configparser": "github:gemini-testing/configparser#TS",
    "glob-extra": "github:gemini-testing/glob-extra#TS",
    "lodash": "^4.17.15",
    "looks-same": "github:gemini-testing/looks-same#FEI-17919",
    "micromatch": "^4.0.2",
    "png-img": "github:gemini-testing/png-img#TS",
    "sizzle": "^2.3.3",
    "temp": "^0.8.3",
    "uglify-js": "^2.8.29",
    "uglifyify": "^3.0.4",
    "yallist": "^3.0.3"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/bluebird": "^3.5.36",
    "@types/browserify": "^12.0.37",
    "@types/color-diff": "^1.2.1",
    "@types/debug": "^4.1.7",
    "@types/lodash": "^4.14.178",
    "@types/micromatch": "^4.0.2",
    "@types/node": "^8.10.66",
    "@types/sizzle": "^2.3.3",
    "@types/temp": "^0.9.1",
    "@types/webdriverio": "^5.0.0",
    "@types/yallist": "^4.0.1",
    "app-module-path": "^1.1.0",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.3.0",
    "doctoc": "^1.0.0",
    "eslint": "^3.1.1",
    "eslint-config-gemini-testing": "^2.2.0",
    "mocha": "^7.1.2",
    "npm-run-all": "^4.1.5",
    "proxyquire": "^1.7.3",
    "sinon": "^2.2.0",
    "typescript": "^4.5.5"
  }
}
